        {
            "botId": "<bot id>",
            "canUpdateConfiguration": false,
            "commands": [
                {
                    "id": "Search",
                    "title": "Search",
                    "description": "",
                    "initialRun": true,
                    "parameters": [
                        {
                            "name": "keyword",
                            "title": "keyword",
                            "description": "search for a sticker"
                        }
                    ]
                }
            ]
        }
    ],
    "permissions": [],
    "validDomains": []
